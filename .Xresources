!!URxvt.background: rgba:1111/1111/1111/dddd
!URxvt.background: [65]#000000
!URxvt.font: xft:Ricty-Bold-iso10646-1
!URxvt.font: xft:Source Code Pro:pixelsize=14,style=medium
!URxvt.font: xft:Inxonsolata-10

urxvt.geometry: 80x26
urxvt.saveLines: 512
urxvt.scrollBar: false
urxvt.termName: rxvt

!urxvt.iconFile: /usr/share/icons/Humanity/apps/128/gnome-terminal.svg
urxvt.font: xft:Ricty:Bold:size=10:antialias=true

! color
urxvt.depth: 32
urxvt.foreground: rgba:ffff/ffff/ffff/ffff
urxvt.background: rgba:0000/0000/0000/cccc

! blue
! from http://blog.asial.co.jp/262
urxvt.color4: #5566ff
urxvt.color12: #7799ff

! fadeout urxvt if outfocus
urxvt.fading: 30

! input with IM at cursor position
urxvt.preeditType: OverTheSpot

! Perl extensions from muennich/urxvt-perls
urxvt.perl-lib: /home/tat/.urxvt/urxvt-perls
urxvt.perl-ext-common: clipboard,url-select

urxvt.keysym.M-c:   perl:clipboard:copy
urxvt.keysym.M-v:   perl:clipboard:paste
urxvt.keysym.M-C-v: perl:clipboard:paste_escaped

URxvt.clipboard.copycmd:  xclip -i -selection clipboard
URxvt.clipboard.pastecmd: xclip -o -selection clipboard

URxvt.url-select.button:    1
URxvt.url-select.underline: true
URxvt.url-select.launcher:  /usr/bin/firefox

